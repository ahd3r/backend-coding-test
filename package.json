{
  "name": "backend-coding-test",
  "version": "1.0.0",
  "description": "A coding assessment for Xendit backend interviews",
  "main": "index.ts",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test mocha tests",
    "start": "cross-env NODE_ENV=development ts-node src/index.ts",
    "build": "tsc",
    "start-prod": "npm run build && cross-env NODE_ENV=production node dist/src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahd3r/backend-coding-test.git"
  },
  "keywords": [
    "Xendit"
  ],
  "author": "Ander",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ahd3r/backend-coding-test/issues"
  },
  "homepage": "https://github.com/ahd3r/backend-coding-test#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "express-validator": "^6.12.1",
    "helmet": "^4.6.0",
    "sqlite3": "^5.0.2",
    "typeorm": "^0.2.37",
    "typescript": "^4.3.5",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "husky": "^7.0.1",
    "mocha": "^9.0.3",
    "prettier": "^2.3.2",
    "supertest": "^6.1.6",
    "ts-node": "^10.2.1"
  }
}
